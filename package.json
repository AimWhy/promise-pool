  "files": [
    "dist",
    "*.md"
  ],
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.42",
    "jest": "^28.1.1",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "tsup": "^6.1.2",
    "typescript": "^4.4.4"
  }
